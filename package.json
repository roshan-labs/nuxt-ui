{
  "name": "@roshan-labs/ui",
  "version": "0.9.1",
  "author": {
    "name": "James Gao",
    "email": "gxmari007@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "type": [
        "./dist/runtime/types.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "storybook": "npm run dev:prepare && storybook dev -p 6006",
    "build-storybook": "npm run dev:prepare && storybook build",
    "lint": "eslint src",
    "release": "release-it"
  },
  "homepage": "https://github.com/roshan-labs/ui",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@nuxt/kit": "^3.2.3",
    "@vueuse/nuxt": "^9.13.0",
    "element-plus": "^2.3.1",
    "magic-string": "^0.30.0",
    "nuxt-windicss": "^2.6.0",
    "unplugin": "^1.3.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.2.3",
    "@release-it/conventional-changelog": "^5.1.1",
    "@roshan-labs/eslint-config": "^1.4.0",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/blocks": "^7.0.0",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/vue3": "^7.0.0",
    "@storybook/vue3-vite": "^7.0.0",
    "@types/react": "file:stub/types-react",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.36.0",
    "nuxt": "^3.2.3",
    "release-it": "^15.9.3",
    "storybook": "^7.0.0",
    "vite-plugin-windicss": "^1.8.10"
  }
}
