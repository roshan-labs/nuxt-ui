{
  "name": "@roshan-labs/nuxt-ui",
  "version": "0.1.0",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./windicss": "./dist/runtime/windicss.mjs"
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
