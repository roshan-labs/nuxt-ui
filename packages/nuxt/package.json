{
  "name": "@roshan-ui/nuxt",
  "description": "roshan-ui for Nuxt3",
  "version": "1.0.1",
  "author": {
    "name": "James Gao",
    "email": "gxmari007@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build": "pnpm dev:prepare && nuxt-module-build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vue3 components",
    "element-plus",
    "roshan-ui"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nuxt/kit": "^3.4.1",
    "magic-string": "^0.30.0",
    "nuxt-windicss": "^2.6.1",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.3.0",
    "@nuxt/schema": "^3.4.1",
    "@nuxt/test-utils": "^3.4.1",
    "changelogen": "^0.5.3",
    "element-plus": "^2.3.3",
    "nuxt": "^3.4.1",
    "@roshan-ui/ui": "workspace:^1.0.0",
    "vitest": "^0.30.1"
  },
  "peerDependencies": {
    "element-plus": "^2.3.3",
    "@roshan-ui/ui": "workspace:^1.0.0"
  }
}
